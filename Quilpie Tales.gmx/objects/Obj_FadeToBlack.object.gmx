        effect_create_above(ef_flare, sparkle_x, sparkle_y, random_range(0, 2), c_white);
    }
    if ((Obj_Opal.stopped == true)&amp;&amp; (image_alpha != 0.00)){ //If the rock has stopped bouncing/moving
